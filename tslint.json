{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "prettier": [true, {
      "printWidth": 120,
      "tabWidth": 4
    }],
    "variable-name": [true, "allow-leading-underscore"]
  }
}
